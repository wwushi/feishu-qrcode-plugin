{
  "name": "二维码生成器",
  "description": "将多维表格字段内容生成二维码，并支持自动更新",
  "version": "1.0.0",
  "author": "Your Name",
  "icon": "https://cdn-icons-png.flaticon.com/512/1239/1239387.png",
  "permissions": [
    "bitable:app",
    "bitable:record:read",
    "bitable:record:write"
  ],
  "entry_points": {
    "bitable:field_shortcut": {
      "type": "function",
      "function": {
        "name": "generateQRCode",
        "description": "将字段内容生成二维码",
        "parameters": [
          {
            "name": "params",
            "type": "object",
            "description": "二维码生成参数",
            "required": true,
            "properties": {
              "content": {
                "type": "string",
                "description": "需要生成二维码的数据，可以是文本或链接",
                "required": true
              },
              "size": {
                "type": "number",
                "description": "二维码尺寸，默认200",
                "required": false,
                "default": 200
              },
              "qrColor": {
                "type": "string",
                "description": "二维码颜色，默认黑色",
                "required": false,
                "default": "#000000"
              },
              "bgColor": {
                "type": "string",
                "description": "二维码背景色，默认白色",
                "required": false,
                "default": "#ffffff"
              }
            }
          }
        ],
        "returns": {
          "type": "string",
          "description": "二维码图片的Base64编码"
        }
      }
    }
  }
}